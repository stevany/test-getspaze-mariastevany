1. Pengulangan bintang
<?php 
function bintang(){ 
  for($i=0;$i<=5;$i++){  
    for($k=5;$k>=$i;$k--){  
      echo "  ";  
    }  
    for($j=1;$j<=$i;$j++){  
      echo "*  ";  
  }  
  echo "<br>";  
  }  
  for($i=4;$i>=1;$i--){  
    for($k=5;$k>=$i;$k--){  
      echo "  ";  
    }  
    for($j=1;$j<=$i;$j++){  
      echo "*  ";  
    }  
    echo "<br>";  
  }  
}
bintang();
?>
2. prima, fibonaci, faktorial
<?php
function bilangan_prima(){
  for($i=1;$i<=10;$i++){ 
        $a = 0; 
        for($j=1;$j<=$i;$j++){ 
            if($i % $j == 0){ 
                $a++;
            }
        }
        if($a == 2){
         echo $i.' ';
        }
    }
}
function fibonaci(){
  $angka_sebelumnya=0;
  $angka_sekarang=1;
  
  echo "$angka_sebelumnya $angka_sekarang";
 
  for ($i=0; $i<10; $i++){
    $output = $angka_sekarang + $angka_sebelumnya;
    echo " $output";
  
  
    $angka_sebelumnya = $angka_sekarang;
    $angka_sekarang = $output;
  }
}
function faktorial($bilangan{
  if($bilangan>=1) {
    echo "Faktorial secara ascending:<br>";
         
    for($i=1; $i <= $bilangan; $i++) {
      $faktorial = 1; //set nilai awal
      echo $i. "! = "; 
         
       for($j=$i; $j > 0; $j--) {
         if($j == 1){
            echo " 1 = " .$faktorial;
         } else {
            <br>echo $j ." x ";<br>        
         }
         <br>$faktorial*=$j; //menghitung hasil faktorial
       }
     }
  } 
}
bilangan_prima();
fibonaci();
faktorial(5);
?>  
